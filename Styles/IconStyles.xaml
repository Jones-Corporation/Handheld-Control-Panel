<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:icon="http://metro.mahapps.com/winfx/xaml/iconpacks">
    <Style x:Key="menuButtonIconMaterial" TargetType="icon:PackIconMaterial">
        <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.Accent}"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Margin" Value="10,0,10,0"/>
    </Style>
    <Style x:Key="menuButtonIconFontAwesome" TargetType="icon:PackIconFontAwesome">
        <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.Accent}"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Margin" Value="10,0,10,0"/>
    </Style>
    <Style x:Key="menuButtonIconUnicons" TargetType="icon:PackIconUnicons">
        <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.Accent}"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Margin" Value="10,0,10,0"/>
    </Style>
   

</ResourceDictionary>